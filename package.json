{
  "name": "aproxy",
  "productName": "aproxy",
  "private": true,
  "version": "0.0.1",
  "description": "debugging proxy",
  "author": "Aokoooooo",
  "license": "MIT",
  "main": "dist/main/index.cjs",
  "scripts": {
    "start": "node scripts/watch.mjs",
    "build": "tsc --noEmit -p packages/renderer/tsconfig.json && node scripts/build.mjs && electron-builder",
