{
  "name": "aproxy",
  "productName": "aproxy",
  "private": true,
  "version": "0.0.1",
  "description": "debugging proxy",
  "author": "Aokoooooo",
  "license": "MIT",
  "main": "dist/main/index.cjs",
  "scripts": {
    "start": "npm run build:plugin && node scripts/watch.mjs",
    "build": "npm run build:plugin && tsc --noEmit -p packages/renderer/tsconfig.json && node scripts/build.mjs && electron-builder",
    "build:plugin": "tsc -p packages/plugin/whistle.aoko/tsconfig.json && cp packages/plugin/whistle.aoko/package.json dist/plugin/whistle.aoko/package.json"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "electron-store": "^8.0.1",
    "electron-window-state": "^5.0.3",
    "fs-extra-promise": "^1.0.1",
    "node-forge": "^1.3.1",
    "uuid": "^8.3.2",
    "whistle": "^2.9.23"
  },
  "devDependencies": {
    "@types/fs-extra-promise": "^1.0.10",
    "@types/node-forge": "^1.0.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-react": "^1.3.2",
    "electron": "^18.2.4",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^6.1.0",
    "prettier": "^2.7.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-electron": "^0.4.9",
    "vite-plugin-esmodule": "^1.2.5",
    "vite-plugin-resolve": "^2.1.1"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 7777
  },
  "volta": {
    "node": "16.15.0"
  }
